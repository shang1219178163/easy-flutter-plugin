{
  "name": "easy-flutter-plugin",
	"displayName": "easy-flutter-plugin",
	"description": "flutter plugin *.dart channel's function convert to objc/swift/kotlin/test/main.dart",
	"version": "0.0.1",
	"license": "MIT",
	"repository": {
     "type": "git",
     "url": "git@github.com:shang1219178163/easy-flutter-plugin.git"
    },
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:extension.EasyFlutterPlugin",
		"onLanguage:dart"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.EasyFlutterPlugin",
				"title": "Easy Flutter Plugin"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus && resourceLangId == dart",
					"command": "extension.EasyFlutterPlugin",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}
